{
  "name": "diag-upload-api",
  "version": "1.0.0",
  "type": "module",
  "description": "Diag Upload API",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,json,md}'",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "deploy": "echo 'Deploy script here'",
    "webpack:start": "webpack serve --open",
    "webpack:build": "webpack",
    "debug": "node --inspect-brk=9229 dist/server.js",
    "dev": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --no-deprecation\" next dev",
    "devturbo": "pnpm dev --turbo",
    "devsafe": "rm -rf .next && pnpm dev",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "ci": "payload migrate && pnpm build",
    "payload": "payload",
    "generate:types": "payload generate:types",
    "generate:schema": "payload-graphql generate:schema",
    "standalone-script": "payload run ./src/scripts/standalone-script.ts",
    "generate:importmap": "payload generate:importmap"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@payloadcms/db-mongodb": "^1.7.3",
    "@payloadcms/richtext-lexical": "^0.11.4",
    "@types/helmet": "^0.0.48",
    "@types/multer": "^1.4.12",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.4.0",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "kafkajs": "^1.15.0",
    "mongodb": "^4.1.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.1",
    "redis": "^3.1.2",
    "redlock": "^5.0.0-beta.2",
    "socket.io": "^4.1.3"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.14",
    "@types/express-fileupload": "^1.2.4",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.0.3",
    "@types/kafka-node": "^2.0.9",
    "@types/node": "^18.7.18",
    "@types/redis": "^4.0.10",
    "@types/redlock": "^4.0.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.1",
    "i18n": "^0.15.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "payload": "^2.30.2",
    "prettier": "^2.7.1",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.0.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
