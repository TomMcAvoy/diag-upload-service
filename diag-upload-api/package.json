  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/express": "^4.17.14",
    "@types/express-fileupload": "^1.5.1",
    "@types/jest": "^29.0.3",
    "@types/kafkajs": "^1.8.2",
