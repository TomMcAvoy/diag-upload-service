    "@types/node": "^18.7.18",
    "@types/socket.io": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.5.0",
