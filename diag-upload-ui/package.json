{
  "name": "diag-upload-service",
  "version": "1.0.0",
  "description": "Diagnostic Upload Service",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,json,md}'",
    "build": "tsc",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "deploy": "echo 'Deploy script here'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "node-fetch": "^3.3.1",
    "kafkajs": "^1.15.0",
    "mongodb": "^4.1.0",
    "socket.io": "^4.1.3",
    "redis": "^3.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/express-fileupload": "^1.2.4",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
