{
  "compilerOptions": {
    "target": "es2017",
    "module": "NodeNext",
    "rootDir": "./src", 
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "nodenext",
    "baseUrl": "./",
    "paths": {
      "payload": ["node_modules/payload"]
    }
  },
  "include": ["src/**/*.ts"], 
  "exclude": []
}
